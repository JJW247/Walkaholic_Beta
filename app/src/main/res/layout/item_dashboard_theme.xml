<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="filenameThemeCategoryImage"
            type="com.mapo.walkaholic.data.model.response.FilenameThemeCategoryImageResponse.FilenameThemeCategoryImage" />
    </data>

    <androidx.cardview.widget.CardView
        android:id="@+id/dashboardThemeCV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="4dp"
        app:cardElevation="0dp">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:setImage="@{filenameThemeCategoryImage.themeFilename}" />

    </androidx.cardview.widget.CardView>
</layout>